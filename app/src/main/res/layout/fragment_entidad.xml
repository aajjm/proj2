<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context=".Activities.Registro">

    <EditText
        android:id="@+id/nombre"
        android:drawableLeft="@drawable/ic_person_outline_black_24dp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:hint="Nom"/>+
    <EditText
        android:id="@+id/direccion"
        android:drawableLeft="@drawable/lat"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:hint="Direccion"
        />
    <EditText
        android:id="@+id/NIF"
        android:drawableLeft="@drawable/finn"

        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:hint="NIF"
        />
    <EditText
        android:id="@+id/Correo"
        android:drawableLeft="@drawable/ic_email_black_24dp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:hint="Correo"
        />
    <EditText
        android:id="@+id/altitud"
        android:drawableLeft="@drawable/ic_language_black_24dp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:hint="Altitud"
        /> <EditText
    android:id="@+id/latitud"
    android:drawableLeft="@drawable/ic_language_black_24dp"
    android:layout_width="200dp"
    android:layout_height="wrap_content"
    android:hint="Latitud"
    /> <EditText
    android:id="@+id/contrase単a"
    android:drawableLeft="@drawable/ic_lock_outline_black_24dp"
    android:layout_width="200dp"
    android:layout_height="wrap_content"
    android:hint="Contrase単a"
    /> <EditText
    android:id="@+id/repcontrase単a"
    android:drawableLeft="@drawable/ic_lock_outline_black_24dp"
    android:layout_width="200dp"
    android:layout_height="wrap_content"
    android:hint="Repetir Contrase単a"
    />
    <Button
        android:id="@+id/registrar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Registrar"/>
</LinearLayout>